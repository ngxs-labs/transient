  "name": "@ngxs-labs/transient",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxs-labs/transient.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngxs-labs/transient#readme",
  "bugs": {
